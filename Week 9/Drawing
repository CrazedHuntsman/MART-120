  <head>
    <title>
      Self portrait
      </title>
      <script src="p5.js"></script>
      <script>
      function setup() {
        createCanvas(400, 400);
      }

      function draw() {
        background(220);
        triangle(200,75,105,250,295,250);
        circle(200,150,150);
        square(125,220,150);
        ellipse(155,135,30,20);
        ellipse(240,135,30,20);
        triangle(200,170,180,175,200,180);
        ellipse(200,90,100,40);
        rect(235,195,40,70);
        rect(120,195,40,70);
        line(170,200,220,195);
        point(170,250)
        textSize(17);
        text('self portrait',155,35);
        textSize(12)
        text('Damian Beardslee',290,390)
      }
      </script>
      </head>
